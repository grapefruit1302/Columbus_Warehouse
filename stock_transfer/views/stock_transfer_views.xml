                    <field name="number"/>
                    <field name="date"/>
                    <field name="warehouse_from_id"/>
                    <field name="warehouse_to_id"/>
                    <field name="employee_from_id"/>
                    <field name="employee_to_id"/>
                    
                    <separator/>
                    <filter string="–ß–µ—Ä–Ω–µ—Ç–∫–∏" name="draft" domain="[('state','=','draft')]"/>
                    <filter string="–ü—ñ–¥—Ç–≤–µ—Ä–¥–∂–µ–Ω—ñ" name="confirmed" domain="[('state','=','confirmed')]"/>
                    <filter string="–í–∏–∫–æ–Ω–∞–Ω—ñ" name="done" domain="[('state','=','done')]"/>
                    <filter string="–°–∫–∞—Å–æ–≤–∞–Ω—ñ" name="cancelled" domain="[('state','=','cancelled')]"/>
                    
                    <separator/>
                    <filter string="–ú—ñ–∂ —Å–∫–ª–∞–¥–∞–º–∏" name="warehouse_transfer" domain="[('transfer_type','=','warehouse')]"/>
                    <filter string="–ú—ñ–∂ –ø—Ä–∞—Ü—ñ–≤–Ω–∏–∫–∞–º–∏" name="employee_transfer" domain="[('transfer_type','=','employee')]"/>
                    <filter string="–ó—ñ —Å–∫–ª–∞–¥—É –ø—Ä–∞—Ü—ñ–≤–Ω–∏–∫—É" name="warehouse_to_employee" domain="[('transfer_type','=','warehouse_employee')]"/>
                    <filter string="–í—ñ–¥ –ø—Ä–∞—Ü—ñ–≤–Ω–∏–∫–∞ –Ω–∞ —Å–∫–ª–∞–¥" name="employee_to_warehouse" domain="[('transfer_type','=','employee_warehouse')]"/>
                    
                    <separator/>
                    <filter string="–°—å–æ–≥–æ–¥–Ω—ñ" name="today" 
                            domain="[('date','=',context_today())]"/>
                    <filter string="–¶—å–æ–≥–æ —Ç–∏–∂–Ω—è" name="this_week" 
                            domain="[('date','&gt;=', (context_today() - datetime.timedelta(days=7)).strftime('%Y-%m-%d'))]"/>
                    
                    <group expand="0" string="–ì—Ä—É–ø—É–≤–∞–Ω–Ω—è –∑–∞">
                        <filter string="–°—Ç–∞—Ç—É—Å–æ–º" name="group_state" context="{'group_by':'state'}"/>
                        <filter string="–¢–∏–ø–æ–º –ø–µ—Ä–µ–º—ñ—â–µ–Ω–Ω—è" name="group_type" context="{'group_by':'transfer_type'}"/>
                        <filter string="–î–∞—Ç–æ—é" name="group_date" context="{'group_by':'date'}"/>
                        <filter string="–°–∫–ª–∞–¥–æ–º –≤—ñ–¥–ø—Ä–∞–≤–Ω–∏–∫–æ–º" name="group_warehouse_from" context="{'group_by':'warehouse_from_id'}"/>
                        <filter string="–°–∫–ª–∞–¥–æ–º –æ–¥–µ—Ä–∂—É–≤–∞—á–µ–º" name="group_warehouse_to" context="{'group_by':'warehouse_to_id'}"/>
                    </group>
                </search>
            </field>
        </record>

        <!-- Action –¥–ª—è –ø–µ—Ä–µ–º—ñ—â–µ–Ω—å -->
